public class Ej37 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// haz la matriz transpuesta del ejercicio anterior
		
		int numeros[][]= new int [4][5];
		
		for (int i = 0; i < 4; i++) {
			for (int j = 0; j < 5; j++) {
				numeros[i][j]=(int) ((Math.random()*100)+1);
			}
		}
		
		for (int k = 0; k < 4; k++) {
			for (int l = 0; l < 5; l++) {
				System.out.printf("%02d",numeros[k][l]);
				System.out.print(" ");
			}
			System.out.println();
		}
		
		
		int transpuesta[][]=new int [5][4];
		
	}

}
