import java.util.Scanner;

/**
 * 
 */

/**
 * @author usuario
 *
 */
public class Ej11 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//imprimir y contar los multiplos de 3 desde 1 hasta n
		
		int n;
		int c=0;
		Scanner teclado = new Scanner(System.in);
				
				System.out.println("introduce un numero:");
				n=teclado.nextInt();
				
		for (int i=0; i<=n; i++) {
			if (i%3==0) {
				System.out.println(i);
				c++;
			}
		}
		System.out.println("El numero de multiplos de 3 de "+ n + " es "+ c);
	}

}
